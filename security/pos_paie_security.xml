<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_pos_paie" model="ir.module.category">
        <field name="name">POS Paie</field>
        <field name="description">Gestion de la paie des vendeurs</field>
        <field name="sequence">20</field>
    </record>

    <record id="group_pos_paie_user" model="res.groups">
        <field name="name">Utilisateur Paie</field>
        <field name="category_id" ref="pos_paie.module_category_pos_paie"/>
    </record>

    <record id="group_pos_paie_manager" model="res.groups">
        <field name="name">Manager Paie</field>
        <field name="category_id" ref="pos_paie.module_category_pos_paie"/>
        <field name="implied_ids" eval="[(4, ref('pos_paie.group_pos_paie_user'))]"/>
    </record>
</odoo>
